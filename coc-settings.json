{
  "coc.preferences.formatOnSave": [
    "cpp",
    "hpp",
    "markdown",
    "javascript",
    "html",
    "yaml",
    "json",
    "go",
    "css",
    "ruby"
  ],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pep8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.venvPath": "~/.config/nvim/venv/bin/python3",
  "python.formatting.autopep8Args": [
    "--in-place",
    "--aggressive",
    "--aggressive"
  ],
  "clangd.path": "/home/raghav/.config/coc/extensions/coc-clangd-data/install/10.0.0/clangd_10.0.0/bin/clangd",
  "clangd.enabled": false,
  "go.goplsPath": "/Users/raghav/go/bin/gopls",
  "go.goplsArgs": ["-remote=auto", "-logfile", "/tmp/gopls.log"],
  "solargraph.diagnostics": true,
  "solargraph.autoformat": true,
